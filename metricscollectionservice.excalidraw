    {
      "id": "C3l0kxzp41bZRT_bl2eso",
      "type": "text",
      "x": 27308.705499742227,
      "y": -4196.011182497901,
      "width": 636,
      "height": 396,
      "angle": 0,
      "strokeColor": "#000000",
      "backgroundColor": "transparent",
      "fillStyle": "hachure",
      "strokeWidth": 1,
